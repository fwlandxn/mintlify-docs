{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Zeno Documentation",
  "colors": {
    "primary": "#3B82F6",
    "light": "#3B82F6",
    "dark": "#3B82F6"
  },
  "favicon": "/images/white.png",
  "navigation": {
    "groups": [
      {
        "group": "Bulletin",
        "icon": "megaphone",
        "expanded": true,
        "pages": [
          "features",
          "commands",
          "configuration"
        ]
      },
      {
        "group": "Modules",
        "icon": "settings-2",
        "expanded": true,
        "pages": [
          "moderation",
          "logging",
          "welcome",
          "roblox-verification"
        ]
      },
      {
        "group": "Miscellaneous",
        "icon": "book-minus",
        "expanded": true,
        "pages": [
          "exclusions",
          "safety-flags",
          "troubleshooting"
        ]
      }
    ]
  },
  "logo": "/images/white.png",
  "navbar": {
    "links": [
      {
        "label": "h",
        "href": "/li"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://zenobot.app/x",
      "github": "https://zenobot.app/github",
      "discord": "https://zenobot.app/discord"
    }
  },
  "background": {
    "decoration": "grid"
  },
  "appearance": {
    "default": "dark"
  },
  "fonts": {
    "family": "Poppins"
  },
  "search": {
    "prompt": "Search or ask"
  },
  "metadata": {
    "timestamp": true
  },
  "interaction": {
    "drilldown": true
  },
  "icons": {
    "library": "lucide"
  }
}
